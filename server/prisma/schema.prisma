generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id               Int     @id @default(autoincrement())
  name             String
  email            String  @unique
  favoriteGenre    String
  favoriteArtist   String?
  gender           String
  lookingForGender String
  relationType     String
  personalAnthem   String?
  createdAt        DateTime @default(now())
}
